pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
  print("i â™¥ wellesley")
  music(0)
end

x = 60
x_dir = true
y_dir = true
y = 60
spd = 1
spr_id = 9
count = 0


function _update60()
  count += 1
  if btn(0) then
    x -= spd
    x_dir = true
    if count % 10 == 0 then
      if spr_id != 11 then
        spr_id = 11
      else
        spr_id = 9
      end
    end
  end
  if btn(1) then
    x += spd
    x_dir = false
    if count % 10 == 0 then
      if spr_id != 11 then
        spr_id = 11
      else
        spr_id = 9
      end
    end
  end
  if btn(2) then
    y -= spd
    spr_id = 3
  end
  if btn(3) then
    y += spd
    spr_id = 7
    if count % 10 == 0 then
      if x_dir == true then
      x_dir = false
      else
      x_dir = true
      end
    end
  end

  if btn(0) or btn(1) or
     btn(2) or btn(3) then
   sfx(0)
  else
    sfx(0, -2)
  end

end

function _draw()
  cls(1)
  palt(0, false)
  palt(12, true)
  spr(spr_id, x, y, 2, 2, x_dir)
end
