local text = "byu-i pico-8 club"
local f = 0

function _init()
sfx(0)
end

function _update()
    f+=5
end

function _draw()
    cls()
    local y
    local c
    local x = 128/2 - (#text*4)/2
    for c=1,#text do
        y = sin((x+f)/100) * 4
        color(5)
        print(sub(text,c,c),x,(30-4)+y)
        if c==9 then
        	spr(2,64-2,40+y)
        end
        y = sin((x+10+f)/100) * 2
        color(7)
        print(sub(text,c,c),x,(30-4)+y)
        x = x+4
        if c==9 then
        	spr(1,64-2,40+y)
        end
    end
    
    rspr(3*8,0,64-18,60,f/400,5)
    

end
